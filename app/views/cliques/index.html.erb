<h1>Cliques</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cliques.each do |clique| %>
      <tr>
        <td><%= clique.name %></td>
        <td><%= link_to 'Join', clique %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clique', new_clique_path %>