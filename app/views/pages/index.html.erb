<div class="container page-container">

<h1>Pages</h1>

<table class="table">
  <thead>
    <tr>
      <th>Clique</th>
      <th>Title</th>
      <th>Entries</th>
    </tr>
  </thead>

  <tbody>
    <% @pages.each do |page| %>
      <tr>
        <td><%= page.slambook.clique.name %></td>
        <td><%= link_to "#{page.title}", page_path(page)%></td>
        <td><%= page.entries.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>